<section class="resources">

  <h1>Coding resources</h1>
  
{{{filter-link isFiltered}}}

  <ul>
    {{#each resources}}
    {{> resource-small this}}
    {{/each}}
  </ul>
</section>

<aside class="aside">
  <h3>Recent reviews<h3>
    <ul>
      {{#each reviews}}
      {{{get_review_small this}}}
      {{/each}}
    </ul>
</aside>
